    .arena-icon-cls2 { fill: #ff272700;}
    .arena-icon-cls3 { fill: #ff272700;}
    .arena-icon-cls4 { fill: #ff272700;}
    .arena-icon-cls5 { fill: #ff272700;} 
    .arena-icon-cls6 { fill: #ff272700;}
        
    /* цвет текста "поиск" во время поиска */
