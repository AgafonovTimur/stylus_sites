    /* отображать посередине доску */

body {
    

    /* отображать посередине доску */
    grid-template-columns: [left-gutter] 2.3fr [extra-left] var(--extraColumnLeftOfBoard, 0) [board-layout] min-content [sidebar] minmax(var(--sidebarMinWidth),var(--sidebarMaxWidth))  1fr ;
    
    /* цвет бэкграунда */
    --theme-background-color: #161412;  

    /*цвет плашки со временем */
    .clock-component.clock-white {  background-color:  #272523}; 

    /*цвет плашки со временем */
    .clock-component.clock-black {  background-color:  #313130}; 

    /*цифры на плашке времени белых */
    .clock-component.clock-white, .clock-component.clock-white span { color: #999393}; 

    /*цифры на плашке времени черных */
    .clock-component.clock-black, .clock-component.clock-black span { color: #cccccc};

    /* убрать мелкую стрелку над и под временем */
    .clock-arrow-arrow { border: 0rem };

    /* размер аватарки  */
    .player-component.player-theatre .player-avatar, .player-component.player-theatre .player-avatar>img {
        border-radius: 0.3rem;
        height: 0rem;
        width: 0rem;
         } 

    /* game review (зеленая кпопка) */
    .ui_v5-button-component.ui_v5-button-primary {  --bgColor: #2b2a2a};

    /*Цвет текста на кнопке game review */
    .ui_v5-button-component {  color: #9e9b9b};   
        
    .ui_v5-button-component { --boxShadowHover: #2b2a2a;}
        
    /* убрать зеленый shadow на next arena game */
    .ui_v5-button-component.ui_v5-button-large {--boxShadowHover: currentColor;}

    /* убрать зеленый цвет под кнопкой play */
    .ui_v5-button-component.ui_v5-button-large { --borderHeight: 0rem};

    /* цвет кнопки Play при наведенни на нее (hover) */
    .ui_v5-button-component.ui_v5-button-primary { --bgHoverColor: #2b2a2a;}
    
    /* цвет пинга */
    .connection-signal { background-color: #9e9b9b};

    /* белые съеденные фигуры */
    .captured-pieces-cpiece { background-image: url('https://i123.fastpic.org/big/2024/0210/73/06052df62cde0cd221e6a8bc2dad1a73.png')};

    /* счетчик разницы в количестве фигур */
    .captured-pieces-score { background-image: url("")}

    /*mistakes button text color*/
    .quick-analysis-tally-component .quick-analysis-tally-count.quick-analysis-tally-mistake { color: #9e9b9b};

    /* цвет надписи mistakes */
    .quick-analysis-tally-component .quick-analysis-tally-count.quick-analysis-tally-mistake span { background-color: #16141200};

    /* цвет текста цифр на кнопках game review, blunder, misses*/
    .quick-analysis-tally-component .quick-analysis-tally-count span { color: #9e9b9b};

    /*blunder button text color*/
    .quick-analysis-tally-component .quick-analysis-tally-count.quick-analysis-tally-blunder { color: #9e9b9b};

    /* цвет надписи blunders */
    .quick-analysis-tally-component .quick-analysis-tally-count.quick-analysis-tally-blunder span { background-color: #16141208};

    /* цвет надписи missess */
    .quick-analysis-tally-component .quick-analysis-tally-count.quick-analysis-tally-missedWin span {background-color: #16141200};

    /* убрать зеленый цвет под кнопкой game review */
    .game-over-review-button-component { height: 5.5rem};

    /* цвет текста mistakes в поп апе*/
    .game-over-quick-analysis-tally-component .game-over-quick-analysis-tally-item.game-over-quick-analysis-tally-mistake { color: #9e9b9b};

    /* цвет картинок mistakes на поп апе */
    .icon-background { fill: #9e9b9b}; 

    /* цвет текста blunders в поп апе */
    .game-over-quick-analysis-tally-component .game-over-quick-analysis-tally-item.game-over-quick-analysis-tally-blunder { color: #9e9b9b};

    /* цвет текста misses в поп апе */ 
    .game-over-quick-analysis-tally-component .game-over-quick-analysis-tally-item.game-over-quick-analysis-tally-missedwin { color: #9e9b9b};

    /* цвет картинки misses в поп апе */
    .cls-2 { fill: #9e9b9b};

    /* misses button text color */
    .quick-analysis-tally-component .quick-analysis-tally-count.quick-analysis-tally-missedWin { color: #9e9b9b};

    /* цвет имени игрока в дзен моде*/
    .user-tagline-compact-theatre .user-tagline-compact-username { color: #9e9b9b};

    /* цвет имени не в дзен моде */
    .user-username-white, .user-username-white:hover { color: #9e9b9b};

    /* цвет рейтинга не в дзен моде */
    .rating-score-rating { color: #9e9b9b};

    /* картинка с кубоком победителя */
    .svg-color-icon-icon.svg-color-icon-cup { background-image: url('')};

    /* цвет хода в табличке при анализе */
    .vertical-move-list .black.selected, .vertical-move-list .white.selected { color: #bcbcbc};    

    /* рейтинг + */
    .rating-score-change.rating-score-positive { color: #9e9b9b};

    /* рейтинг - */
    .rating-score-change.rating-score-negative { color: #9e9b9b};
    
    /* текст game review в поп апе */
    .game-over-review-button-label { color: #9e9b9b;} 
        
    /* цвет текста при наведении на next game снизу справа не в дзен моде */
    .ui_v5-button-component.ui_v5-button-primary.ui_v5-button-full { --textHoverColor: #9e9b9b; }
        
    /**/
    .dark-mode .game-over-arena-component .game-over-arena-place span, .dark-mode .game-over-arena-component .game-over-arena-score span  { color: #9e9b9b};
        
    /* размер картинки с мечом и шлемом сбоку в турнире */
    .ui_v5-button-component .ui_v5-button-icon { font-size: 0rem;}

    /*цвет при наведении next arena game */
    .ui_v5-button-component:not(.ui_v5-button-full)>span:not(.ui_v5-button-icon) {color: #9e9b9b}

    .league-badge-component .league-badge-icon {             
        height: 0rem;
        width: 0rem;
    }

    /* цифры рейтинга в лиге */
    .league-badge-component .league-badge-points { font-size: 0rem;}

    /* цифры изменения рейтинга в лиге */
    .league-score-change { font-size: 0rem;}
    
    /*timeout avatar */
    .timeout-avatar-component .timeout-avatar-icon { font-size : 0rem;}
    
    /*  div class="hover-square и дальше номер квадрата - рамка вокруг квадрата на который наводишь мышку*/
    rect{ stroke: #c23232;} /*fill : #c2323229 заливка внутри квадрата*/
    
    /*выделенные квадраты с прошлым ходом */
    .highlight { background-color: #C23232 !important;  opacity: 0.5 !important; }
    
    /* стрелки объяснения */
    .arrow { fill :#E8AB18C4 !important; opacity: .7 !important;}
                
    /* изменение цвета доски при анализе - наложенный оверлей */
    .board.analysis-overlay:before { background: #47474717;}
    
    /* evalutation bar */
    .evaluation-bar-bar .evaluation-bar-white { background-color: #9F9E9E;}
    
    /* значок online или нет в профиле */
    .presence-square-component .presence-square-red { background: #6F1919;}
    .presence-square-component .presence-square-green { background: #105304;}
    
    /* кнопка watch в профиле и при клике на имя во время игры */
    .ui_v5-button-component.ui_v5-button-danger {
    --bgColor: #20231d;
    --bgHoverColor: #20231d;
    --secondaryBorderColor: #20231d;
    --borderBottomLine: #20231d;
    --borderBottomLineHover: #20231d;
    --innerBoxShadow: rgba(224, 40, 40, 0);
    }
    
    /* кнопка challenge в профиле */
    .presence-button-component .staffGreen { background: #20231d;}

}

