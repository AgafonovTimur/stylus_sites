    /* all layout */

    /* ❗❗❗ !important - overwrite functions */

body {
    /* рейтинг + */
    .rating-score-change.rating-score-positive { color: #161412 !important; }

    /* рейтинг - */
    .rating-score-change.rating-score-negative { color: #161412 !important; }
    
    /* цвет имени игрока в дзен моде*/
    .user-tagline-compact-theatre .user-tagline-compact-username { color: #161412 !important};

    /* цвет имени не в дзен моде */
    .user-username-white, .user-username-white:hover { color: #161412 !important};
    
    /* цвет рейтинга не в дзен моде */
    .rating-score-rating { color: #161412 !important};
    
    /* картинка времени партии: пуля, блитц и тд */
    .svg-color-icon-icon.svg-color-icon-bullet, .svg-color-icon-icon.svg-color-icon-lightning { background-image: url(''); }  
    
    /* цвет плашки и места в рейтинге не в дзен моде */
    .arena-score-component { background-color: #161412 !important ; color: #161412 !important; }
    .arena-score-component .arena-score-place { color: #161412 !important; }
    
    /* флаг страны */ 
    .country-flag-small, .country-flags-component { background-image: url(''); }
        
    /* цифры в ранке */
    .game-over-arena-component .game-over-arena-place small.game-over-arena-negative, .game-over-arena-component .game-over-arena-score small.game-over-arena-negative {color: #262421 !important;}
    
    /* цифры в поп апе победы */
    .game-over-arena-component .game-over-arena-place small, .game-over-arena-component .game-over-arena-score small { color: #262421 !important; }
    
    /* show your apreciation */
    .flair-component { background: url('') !important; }
}

    /* цифры в поп апе победы */
    .dark-mode .game-over-arena-component .game-over-arena-place strong, .dark-mode .game-over-arena-component .game-over-arena-score strong {color: #262421 !important ;}
    /* points в поп апе на арене */
    .dark-mode .game-over-arena-component .game-over-arena-place span, .dark-mode .game-over-arena-component .game-over-arena-score span { color: #262421 !important; }



