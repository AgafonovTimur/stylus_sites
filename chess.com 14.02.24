    /* Закрыть поп ап окончания игры на арене */
    .dark-mode .game-over-header-component .game-over-header-close { color: #9e9b9b;}

    /**/
    .dark-mode .game-over-header-component.game-over-header-whiteWon { color: #9e9b9b;}

    /* evalutaion bar цвет полосок у хода */
    .dark-mode .vertical-move-list .time-white:after { background-color: #9e9b9b; }

        
        
        
